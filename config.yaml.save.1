printerParameters: {}
printerProfiles:
  defaultProfile:
    axes:
      e:
        inverted: false
        speed: 500
      x:
        inverted: false
        speed: 18000
      y:
        inverted: false
        speed: 18000
      z:
        inverted: false
        speed: 18000
    color: blue
    extruder:
      count: 1
      nozzleDiameter: 0.5
      offsets:
      - - 0.0
        - 0.0
    heatedBed: true
    id: _default
    model: Delta
    name: Rostock
    volume:
      depth: 340.0
      formFactor: circular
      height: 240.0
      width: 240.0
serial:
  autoconnect: true
  baudrate: 250000
  port: /dev/ttyACM0
  timeout: {}
server:
  firstRun: false
  secretKey: PPAIyzqOPvsL5W92vjLlUZhpjetFDtHk
system:
  actions:
  - action: copy
    command: /home/pi/scripts/SmoothieUpload.sh
    confirm: true
    name: Copy to SD(Smoothie Mount)
  - action: shutdown
    command: sudo shutdown -h now
    confirm: You are about to shutdown the system.
    name: Shutdown
  - action: reboot
    command: sudo shutdown -r now
    confirm: You are about to reboot the system
    name: Reboot
  - action: restart
    command: sudo service octoprint restart
    confirm: You are about to restart OctoPrint
    name: Restart OctoPrint
  - action: streamon
    command: /home/pi/scripts/webcam start
    confirm: false
    name: Start video stream
  - action: streamoff
    command: /home/pi/scripts/webcam stop
    confirm: false
    name: Stop video stream
temperature:
  profiles:
  - bed: 100
    extruder: '230'
    name: ABS
  - bed: 60
    extruder: 180
    name: PLA
  - bed: '115'
    extruder: '240'
    name: ABS Hot
webcam:
  ffmpeg: /usr/bin/avconv
  snapshot: http://127.0.0.1:8080/?action=snapshot
  stream: /webcam/?action=stream
  timelapse:
    fps: 25
    options:
      interval: 10
    postRoll: 10
    type: timed
  watermark: false
